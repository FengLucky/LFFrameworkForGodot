<?xml version="1.0"?>
<module
        xmlns="enum"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="enum ~enum.xsd"
        name="">
    <enum name="PageType" comment="界面类型">
        <var name="Cover" alias="覆盖" comment="铺满整个屏幕且遮盖下层界面"/>
        <var name="Popup" alias="弹出" comment="占据屏幕一部分"/>
        <var name="Transparent" alias="透明" comment="铺满整个屏幕但有透明部分无法遮盖下层界面"/>
    </enum>
    
    <enum name="PageLayer" comment="界面层级">
        <var name="UI" comment="普通 UI 层级"/>
        <var name="Loading" comment="加载界面"/>
        <var name="Toast" comment="提示信息,不可交互"/>
        <var name="Dialog" comment="确认框"/>
        <var name="Count"/>
    </enum>
</module>